/*Global*/
::selection{
	background-color: green;
}

:root{
	box-sizing: border-box;
}
*,::before, ::after{
	box-sizing: inherit;
}
*{
	font-family: 'corbel';
}
.button{
	color: white;
	padding: 1em;
	border: 0;
	border-radius: 1em;
	background-color: rgb(32, 46, 34);
	box-shadow: 5px 5px 3px 1px rgb(20, 34, 22),
	-5px -5px 5px 1px rgb(44, 58, 46);
}
.button:hover{
	background-color: rgb(44, 58, 46);
}

::-webkit-scrollbar {
	width: 13px;
}

::-webkit-scrollbar-track {
	background-color: rgb(127, 177, 127);
}

::-webkit-scrollbar-thumb {
	background: rgb(44, 58, 46);
	border-radius: 1em;
	box-shadow: inset 2.5px 2.5px 2.5px 1.5px rgb(20, 34, 22);
}

::-webkit-scrollbar-thumb:hover {
	box-shadow: inset 2.5px 2.5px 2.5px 1.5px rgb(20, 34, 22);
	background: rgb(20, 34, 22);
}
/***********************


/*Home page*/
*{
	transition: .3s;
}
body{
	overflow-x: hidden;
	margin: 0;
	/* border: 3px solid pink; */
	width: 100vw;
	background-image: url('/img/background2.png');
	background-position: center;
	background-size: 70%;
	background-repeat: no-repeat;
	background-attachment: fixed;
	background-color: rgba(0, 100, 0, .5);
	color: white;
}
a{
	text-decoration: none;
	color: white;
}

.head{
	position: sticky;
	display: block;
	/*box-shadow: 0 .4em 1em .4em rgba(32, 46, 34, .9);*/
	/*margin-top: 0;*/
	/*border: 3px solid white;*/
	padding: .4em;
	background-color: rgb(32, 46, 34);
	background-color: rgb(32, 46, 34);
	height: fit-content;
}
.logins > *{
	display: inline-block;
}
.logins{
	/*border: 3px solid red;*/
	margin-top: -5em;
	text-align: center;
	float: right;
}
.logins > .button{
	margin-left: 1.5em;
}


.footer{
	display: block;
	background-color: rgb(32, 46, 34);
	position: fixed;
	width: 100vw;
	padding: .4em;
	bottom: 0;
}
/************************/


/*Sign in page*/
.signin_div{
	width: fit-content;
	margin: auto;
}

.form{
	position: absolute;
	top: 50%;
	left: 50%;
	-ms-transform: (-50%, -50%);
	transform: translate(-50%, -50%);
	height: fit-content;
	margin: 0;
	text-align: center;	

	padding: 1em;
	width: fit-content;
	margin: auto;
	background-color: rgba(32, 46, 34, .8);
	text-align: center;
}
.form > *:not(label){
	color: white;
	width: calc(20vw + 20em);
	margin: 1em;
	padding: 1em;
	border-width: 0;
	border-radius: 1em;
	box-shadow: 5px 5px 3px 1px rgb(20, 34, 22),
	-5px -5px 5px 1px rgb(44, 58, 46);
}
.form a{
	display: block;
}
.input:invalid{
	background-color: rgb(100, 50, 50);
}
.input{
	background-color: rgb(82, 96, 84);
	
}

