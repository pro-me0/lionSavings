/*Menu*/
body{
	display: flex;
	/*border: 3px solid white;*/
	position: relative;
}
.menub{
	background-color: rgba(0, 0, 0, .1);
	position: fixed;
	right: 1em;
	border: none;
	font-size: 2em;
	line-height: .5;
	width: 1em;
	height: 1em;
	color: white;
	z-index: 4;
	cursor: pointer;
	top: 1em;
	/*border-radius: .5em;*/
}
.menu > *{
	font-size: 0;
	padding: 1em;
	margin: .5em;
}
.menu > *:nth-of-type(odd){
	background-color: rgba(0, 0, 0, .2);
}
.menu{
	font-size: 1.2em;
	display: none;
	height: 100vh;
	background-color: rgba(20, 34, 22, 1);
	/* margin: 1em; */
	/* display: inline-flexbox; */
}
.settings>*{
	display: block;
}
.body{
	/* display: inline-flexbox; */
	width: 100vw;
}
.body, .menu{
	vertical-align: top;
	/*border: 2px solid red;*/
	display: inline-block;
}


/************************/


/*Settings page*/
.set{
	display: block;
	padding: 1em;
	margin: .5em;
}
.set > *{
	width: inherit;
}
.set:nth-of-type(odd){
	background-color: rgba(0, 0, 0, .2);
}
