/*Show Page*/
.profile_index{
	/*border: 3px solid wheat;*/
	width: fit-content;
	margin: auto;
}

.profile_index > *{
	min-width: 25.625em;
	max-width: 44.625em;
}

.profile{
	/*border: 3px solid black;*/
	text-align: center;
}
.profile > *{
	background-color: rgba(20, 34, 22, .9);
	display: inline-block;
	border-radius: 1em;
	vertical-align: middle;
	height: 150px;
	min-width: 20em;
	max-width: 30em;
	padding: .5em;
	margin: .5em;
}
.other{
	/*border: 3px solid black;*/
	/*height: 50vh;*/
	text-align: center;
	/*width: 90vw;*/
	margin: auto;
}
.other > *{
	min-width: 20em;
	height: 150px;
	border-radius: 1em;
	max-width: 30em;
	background-color: rgba(20, 34, 22, .9);
	padding: 3.5em;
	margin: .5em;
	display: inline-block;
}
